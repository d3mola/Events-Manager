{
  "name": "events-manager",
  "description": "manage applications to your event facilities",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "build:server": "./node_modules/.bin/babel ./server -d dist",
    "build:client": "webpack -p --config webpack.config.prod.js",
    "eslint": "./node_modules/.bin/eslint ./server/app.js",
    "nodemon": "nodemon ./server/app",
    "test:db": "cross-env NODE_ENV=test sequelize db:migrate:undo:all &&cross-env NODE_ENV=test sequelize db:migrate",
    "test:server": "npm run test:db &&cross-env NODE_ENV=test nyc --reporter=lcov --reporter=html --reporter=text mocha --compilers js:babel-core/register ./server/test/**/*.spec.js --timeout 250000 --exit",
    "test:client": "jest client/",
    "test:client:watch": "jest client/ --watch",
    "test:client:coverage": "jest client/ --coverage",
    "jest": "jest client/ --coverage --watch",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "seed:create": "sequelize seed:generate --name",
    "seed": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo:all",
    "coverage": "NODE_ENV=test nyc report --reporter=text-lcov | coveralls",
    "start": "node ./dist/app.js",
    "start:client": "webpack-dev-server --hot --inline --config webpack.config.dev.js",
    "start:server": "nodemon ./server/app --exec babel-node",
    "heroku-postbuild": "rm -rf dist && npm run build:client && npm run build:server",
    "heroku-api": "./node_modules/.bin/babel ./server -d dist && node dist/app.js"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "dist"
    ]
  },
  "jest": {
    "setupFiles": [
      "./client/__test__/jestsetup.js"
    ],
    "setupTestFrameworkScriptFile": "./client/utils/localStorageMock",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "coverageDirectory": "<rootDir>/client/coverage"
  },
  "engines": {
    "node": "8.11.1",
    "npm": "5.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d3mola/Events-Manager.git"
  },
  "keywords": [
    "express",
    "postgres",
    "sequelize",
    "node",
    "react",
    "redux",
    "redux-saga"
  ],
  "author": "Ademola Ariya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/d3mola/Events-Manager/issues"
  },
  "homepage": "https://github.com/d3mola/Events-Manager#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0-alpha.6",
    "cloudinary": "^1.11.0",
    "connect-history-api-fallback": "^1.5.0",
    "cors": "^2.8.4",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "file-loader": "^1.1.5",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^8.1.0",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.12.1",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "popper.js": "^1.12.9",
    "prop-types": "^15.6.0",
    "rc-pagination": "^1.16.3",
    "rc-select": "^8.0.9",
    "react": "^16.1.1",
    "react-calendar": "^2.15.1",
    "react-dom": "^16.1.1",
    "react-loader-spinner": "^1.4.7",
